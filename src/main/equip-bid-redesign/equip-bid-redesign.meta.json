{
  "UserScript": {
    "name": [{ "value": "Equip-Bid Redesign" }],
    "namespace": [{ "value": "https://github.com/bricemciver/GreasemonekeyScripts" }],
    "description": [{ "value": "Enhancements to the Equip Bid auction website" }],
    "license": [{ "value": "MIT" }],
    "version": [{ "value": "0.1" }],
    "match": [{ "value": "https://www.equip-bid.com/*" }],
    "icon": [{ "value": "https://www.google.com/s2/favicons?sz=64&domain=equip-bid.com" }],
    "grant": [{ "value": "GM_xmlhttpRequest" }, { "value": "GM.xmlHttpRequest" }],
    "connect": [{ "value": "equip-bid.com" }]
  }
}
