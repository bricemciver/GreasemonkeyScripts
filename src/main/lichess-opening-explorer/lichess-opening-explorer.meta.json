{
  "UserScript": {
    "name": [{ "value": "Lichess Opening Explorer" }],
    "namespace": [{ "value": "https://github.com/bricemciver/GreasemonekeyScripts" }],
    "description": [{ "value": "Show master openings for the current position" }],
    "license": [{ "value": "MIT" }],
    "version": [{ "value": "0.1" }],
    "grant": [{ "value": "GM.xmlHttpRequest" }],
    "connect": [{ "value": "explorer.lichess.ovh" }],
    "match": [{ "value": "https://lichess.org/*" }],
    "icon": [{ "value": "https://icons.duckduckgo.com/ip3/lichess.org.ico" }]
  }
}
