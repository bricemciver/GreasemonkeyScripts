{
  "UserScript": {
    "name": [{ "value": "FMHY Base64 Auto Decoder" }],
    "version": [{ "value": "2.4" }],
    "author": [{ "value": "Rust1667" }],
    "description": [{ "value": "Decode base64-encoded links in some pastebins and make URLs clickable" }],
    "match": [
      { "value": "*://rentry.co/*" },
      { "value": "*://rentry.org/*" },
      { "value": "*://pastes.fmhy.net/*" },
      { "value": "*://bin.disroot.org/?*#*" },
      { "value": "*://privatebin.net/?*#*" },
      { "value": "*://textbin.xyz/?*#*" },
      { "value": "*://bin.idrix.fr/?*#*" },
      { "value": "*://privatebin.rinuploads.org/?*#*" },
      { "value": "*://pastebin.com/*" }
    ],
    "grant": [{ "value": "none" }],
    "icon": [
      { "value": "https://t1.gstatic.com/faviconV2?client=SOCIAL&type=FAVICON&fallback_opts=TYPE,SIZE,URL&url=http://fmhy.net&size=64" }
    ],
    "namespace": [{ "value": "https://greasyfork.org/users/980489" }],
    "downloadURL": [{ "value": "https://update.greasyfork.org/scripts/485772/FMHY%20Base64%20Auto%20Decoder.user.js" }],
    "updateURL": [{ "value": "https://update.greasyfork.org/scripts/485772/FMHY%20Base64%20Auto%20Decoder.meta.js" }]
  }
}
