{
    "UserScript": {
      "name": [{ "value": "Amazon - Hide Sponsored" }],
      "namespace": [{ "value": "https://github.com/bricemciver/GreasemonekeyScripts" }],
      "description": [{ "value": "Hide sponsored items from search results" }],
      "license": [{ "value": "MIT" }],
      "version": [{ "value": "0.0.1" }],
      "match": [
        { "value": "https://amazon.com/*" },
        { "value": "https://*.amazon.com/*" },
        { "value": "https://amazon.co.uk/*" },
        { "value": "https://*.amazon.co.uk/*" },
        { "value": "https://amazon.ca/*" },
        { "value": "https://*.amazon.ca/*" },
        { "value": "https://amazon.de/*" },
        { "value": "https://*.amazon.de/*" },
        { "value": "https://amazon.fr/*" },
        { "value": "https://*.amazon.fr/*" },
        { "value": "https://amazon.es/*" },
        { "value": "https://*.amazon.es/*" },
        { "value": "https://amazon.it/*" },
        { "value": "https://*.amazon.it/*" },
        { "value": "https://amazon.co.jp/*" },
        { "value": "https://*.amazon.co.jp/*" },
        { "value": "https://amazon.cn/*" },
        { "value": "https://*.amazon.cn/*" },
        { "value": "https://amazon.com.br/*" },
        { "value": "https://*.amazon.com.br/*" },
        { "value": "https://amazon.in/*" },
        { "value": "https://*.amazon.in/*" },
        { "value": "https://amazon.com.mx/*" },
        { "value": "https://*.amazon.com.mx/*" },
        { "value": "https://amazon.com.au/*" },
        { "value": "https://*.amazon.com.au/*" }
      ],
      "icon": [{ "value": "https://www.google.com/s2/favicons?sz=64&domain=amazon.com" }]
    }
  }
  